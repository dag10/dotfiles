{
    "editor.rulers": [79],
    "editor.minimap.enabled": true,
    "workbench.iconTheme": "vscode-icons",
    "editor.lineNumbers": "relative",
    "editor.renderWhitespace": "all",
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.macOptionClickForcesSelection": true,
    "cmake.options.statusBarVisibility": "visible",
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "cmake.debugConfig": {
        "stopAtEntry": false,
        "MIMode": "lldb",
        "miDebuggerPath": "/Users/drew/.vscode/extensions/ms-vscode.cpptools-1.20.5-darwin-arm64/debugAdapters/lldb-mi/bin/lldb-mi",
        "logging": {
            "trace": true,
            "engineLogging": true,
            "traceResponse": true
        }
    },
    "cmake.showOptionsMovedNotification": false,
    "git.openRepositoryInParentFolders": "always",
    "workbench.editor.wrapTabs": true,
    "window.density.editorTabHeight": "compact",
    "git-graph.repository.onLoad.scrollToHead": true,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.localEchoEnabled": "off",
    "terminal.integrated.env.osx": {
        "TERM": "ansi"
    },
    "vs-kubernetes": {
        "vscode-kubernetes.helm-path-mac": "/Users/drew/.vs-kubernetes/tools/helm/darwin-arm64/helm",
        "vscode-kubernetes.minikube-path-mac": "/Users/drew/.vs-kubernetes/tools/minikube/darwin-arm64/minikube",
        "vscode-kubernetes.kubectl-path-mac": "/Users/drew/.vs-kubernetes/tools/kubectl/kubectl"
    },
    "terminal.integrated.defaultProfile.osx": "fish",
    "terminal.integrated.localEchoStyle": "#00ff00",
    "terminal.integrated.fontFamily": "MesloLGMDZ Nerd Font Mono",
    "git-graph.dialog.rebase.launchInteractiveRebase": true,
    "git.confirmSync": false,
    "[cpp]": {
        "editor.wordBasedSuggestions": "off",
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true
    },
    "terminal.integrated.macOptionIsMeta": true,
    "window.autoDetectColorScheme": true,
    "git.enableSmartCommit": true,
    "editor.minimap.showSlider": "always",
    "editor.minimap.size": "fit",
    "editor.minimap.renderCharacters": false,
    "editor.scrollbar.vertical": "hidden",
    "scm.experimental.showHistoryGraph": true,
    "workbench.startupEditor": "none",
    "cmake.configureOnOpen": true,
    "vsicons.dontShowNewVersionMessage": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
